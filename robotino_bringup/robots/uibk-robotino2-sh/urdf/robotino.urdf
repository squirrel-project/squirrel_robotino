    <origin rpy="1.57297 -3.11833 3.10173" xyz="0.0772857 0.117459 -0.0273189"/>
    <parent link="tilt_link"/>
    <child link="kinect_link"/>
  </joint>
  <link name="kinect_link">
    <inertial>
      <mass value="0.00001"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.035 0.18 0.02"/>
      </geometry>
      <material name="Grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.035  0.18 0.02"/>
      </geometry>
    </collision>
